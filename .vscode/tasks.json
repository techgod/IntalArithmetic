{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "shell: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"--std=c99",
				"-Wall",
				"custom_test.c",
				"${file}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			  }
		}
	]
}